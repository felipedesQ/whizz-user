{
    "name": "felipedesq/shared-object",
    "description": "Shared domain files and object",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "doctrine/doctrine-bundle": "1.11.2",
        "ramsey/uuid": "^3.8",
        "symfony/console": "^4.1",
        "ext-json": "*",
        "ext-intl": "*",
        "guzzlehttp/guzzle": "~6.0",
        "symfony/monolog-bundle": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "phpspec/phpspec": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Whizz\\SharedObject\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Whizz\\SharedObject\\UnitTests\\": "tests/units/"
        }
    }
}
